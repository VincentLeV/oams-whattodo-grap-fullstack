{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "knex:init": "knex migrate:make init --migrations-directory db/migrations",
    "migrate": "npx knex migrate:latest --knexfile db/knexfile.js",
    "migrate:prod": "npx knex migrate:latest --knexfile db/knexfile.js --env production",
    "migrate:test": "npx knex migrate:latest --knexfile db/knexfile.js --env test",
    "migrate:rb": "npx knex migrate:rollback --knexfile db/knexfile.js --env test",
    "test": "cross-env NODE_ENV=test npm run migrate:rb && npm run migrate:test && jest --verbose --runInBand --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "apollo-server-express": "^3.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "graphql": "^16.3.0",
    "install": "^0.13.0",
    "knex": "^1.0.3",
    "npm": "^8.5.1",
    "pg": "^8.7.3",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15"
  }
}
